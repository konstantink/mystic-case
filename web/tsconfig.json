{
    "compilerOptions": {
        "baseUrl": "./",
        "target": "es5",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "sourceMap": true,
        "strictNullChecks": false,
        "isolatedModules": true,
        "noEmit": false,
        "jsx": "react-jsx",
        // "paths": {
        //   "@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"]
        // },
        "paths": {
            "@mysticcase/services/auth": ["src/services/auth/index.ts"],
            "@mysticcase/storage-helper": ["src/services/storage/index.ts"],
            "@mysticcase/*": ["src/*"],
        }
    },
    "awesomeTypescriptLoaderOptions": {
        /* ... */
    },
    "include": [
        "src",
    ],
    "exclude": [
        "node_modules",
        "dist",
    ]
}
